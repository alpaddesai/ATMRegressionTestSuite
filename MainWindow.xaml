        <Button x:Name="ButtonFour" Content="4" HorizontalAlignment="Left" Margin="7,168,0,0" VerticalAlignment="Top" Width="46" Height="32" Click="ButtonFour_Click" Grid.Column="2" Grid.ColumnSpan="3" Grid.Row="1">
