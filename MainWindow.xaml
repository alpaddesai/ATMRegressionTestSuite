        </Button>
        <Button x:Name="ButtonTen" Content="0" HorizontalAlignment="Left" Margin="7,270,0,0" VerticalAlignment="Top" Width="46" Height="32" Click="ButtonTen_Click" Grid.Column="2" Grid.ColumnSpan="3" Grid.Row="1">
            <Button.Background>
                <ImageBrush ImageSource="KeypadButton.jpg"/>
            </Button.Background>
        </Button>
        <Button x:Name="ButtonEnter" Content="Enter" HorizontalAlignment="Left" Margin="34,270,0,0" VerticalAlignment="Top" Width="113" Height="32" Click="ButtonEnter_Click" Grid.Column="4" Grid.ColumnSpan="2" Grid.Row="1">
            <Button.Background>
                <ImageBrush ImageSource="KeypadButton.jpg"/>
            </Button.Background>
        </Button>
        <TextBox Grid.Column="1" HorizontalAlignment="Left" Height="124" Margin="19,0,0,253" TextWrapping="Wrap" Text="Initial Text" VerticalAlignment="Bottom" Width="653" Grid.ColumnSpan="13" Grid.RowSpan="2" Name ="Screen" TextChanged="TextBox_TextChanged"/>
        <Label Content="Reference: How to Program Dietel C#" Grid.Column="5" HorizontalAlignment="Left" Margin="88,-21,0,0" VerticalAlignment="Top" Width="247" Grid.ColumnSpan="7" Height="26"/>
        <Button x:Name="CustomGUI" Content="Alpa's CustomGUI Launch Regression TestSuite" Grid.Column="10" HorizontalAlignment="Left" Margin="0,5,0,0" VerticalAlignment="Top" Width="261" Grid.ColumnSpan="4" Height="25" Click="CustomGUI_Click"/>
    </Grid>
</Window>
