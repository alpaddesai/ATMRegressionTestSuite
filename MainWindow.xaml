<Window x:Class="AlpaATMGUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AlpaATMGUI"
        mc:Ignorable="d"
        Title="MainWindow" Height="452" Width="702" ResizeMode="NoResize">
    <Grid Margin="0,0,2,0" Background="#FFEDEDED">
        <Grid.RowDefinitions>
            <RowDefinition Height="94"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="0*"/>
            <ColumnDefinition Width="45*"/>
            <ColumnDefinition Width="41*"/>
            <ColumnDefinition Width="0*"/>
            <ColumnDefinition Width="38*"/>
            <ColumnDefinition Width="212*"/>
            <ColumnDefinition Width="0*"/>
            <ColumnDefinition Width="64*"/>
            <ColumnDefinition Width="0*"/>
            <ColumnDefinition Width="12*"/>
            <ColumnDefinition Width="44*"/>
            <ColumnDefinition Width="22*"/>
            <ColumnDefinition Width="95*"/>
            <ColumnDefinition Width="119*"/>
        </Grid.ColumnDefinitions>
        <Rectangle HorizontalAlignment="Left" Height="235" Margin="19,80,0,0" Stroke="Black" VerticalAlignment="Top" Width="233" Grid.ColumnSpan="6" Grid.Row="1">
            <Rectangle.OpacityMask>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="Black" Offset="0"/>
                    <GradientStop Color="White" Offset="1"/>
                </LinearGradientBrush>
            </Rectangle.OpacityMask>
            <Rectangle.Fill>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="Black" Offset="0"/>
                    <GradientStop Color="White" Offset="1"/>
                </LinearGradientBrush>
            </Rectangle.Fill>
        </Rectangle>
        <Button x:Name="ButtonWithdrawl" Content="Withdrawl Cash" HorizontalAlignment="Left" Margin="141,80,0,0" VerticalAlignment="Top" Width="405" Height="109" Click="ButtonWithdrawl_Click" AutomationProperties.IsOffscreenBehavior="FromClip" Grid.Column="5" Grid.Row="1" Grid.ColumnSpan="9">
            <Button.Background>
                <ImageBrush ImageSource="TakeCashHereButtonImage.jpg"/>
            </Button.Background>
        </Button>
        <Button x:Name="ButtonDeposit" Content="Deposit Cash" HorizontalAlignment="Left" Margin="141,207,0,0" VerticalAlignment="Top" Width="405" Height="108" RenderTransformOrigin="0.284,1.978" Click="ButtonDeposit_Click" AutomationProperties.IsOffscreenBehavior="FromClip" Grid.Column="5" Grid.Row="1" Grid.ColumnSpan="9">
            <Button.Background>
                <ImageBrush ImageSource="TakeCashHereButtonImage.jpg"/>
            </Button.Background>
        </Button>
        <Button x:Name="ButtonOne" Content="1" HorizontalAlignment="Left" Margin="7,118,0,0" VerticalAlignment="Top" Width="46" Height="32" Click="ButtonOne_Click" Grid.Column="2" Grid.ColumnSpan="3" Grid.Row="1">
            <Button.Background>
                <ImageBrush ImageSource="KeypadButton.jpg"/>
