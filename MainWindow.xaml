        <Button x:Name="ButtonThree" Content="3" HorizontalAlignment="Left" Margin="63,118,0,0" VerticalAlignment="Top" Width="46" Height="32" Click="ButtonThree_Click" Grid.Column="5" Grid.Row="1">
            <Button.Background>
                <ImageBrush ImageSource="KeypadButton.jpg"/>
            </Button.Background>
        </Button>
        <Button x:Name="ButtonFive" Content="5" HorizontalAlignment="Left" Margin="34,168,0,0" VerticalAlignment="Top" Width="46" Height="32" Click="ButtonFive_Click" Grid.Column="4" Grid.ColumnSpan="2" Grid.Row="1">
