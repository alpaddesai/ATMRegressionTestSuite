        <Button x:Name="ButtonSix" Content="6" HorizontalAlignment="Left" Margin="63,168,0,0" VerticalAlignment="Top" Width="46" Height="32" Click="ButtonSix_Click" Grid.Column="5" Grid.Row="1">
            <Button.Background>
