        </Button>
        <Button x:Name="ButtonSeven" Content="7" HorizontalAlignment="Left" Margin="7,218,0,0" VerticalAlignment="Top" Width="46" Height="32" Click="ButtonSeven_Click" Grid.Column="2" Grid.ColumnSpan="3" Grid.Row="1">
            <Button.Background>
                <ImageBrush ImageSource="KeypadButton.jpg"/>
            </Button.Background>
        </Button>
        <Button x:Name="ButtonEight" Content="8" HorizontalAlignment="Left" Margin="34,218,0,0" VerticalAlignment="Top" Width="46" Height="32" Click="ButtonEight_Click" Grid.Column="4" Grid.ColumnSpan="2" Grid.Row="1">
